	color: #666;
}
.jb_text{
	width: 100%;
	background: #eee;
	border-radius: 10rpx;
	padding: 20rpx;
	box-sizing: border-box;
	font-size: 24rpx;
}
.iconnext{
	font-size: 24rpx;
}
