	width: 80%;
	height: 80rpx;
	border-radius: 8rpx;
	margin: 200rpx auto 10rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	background: #3171F5;
	color: #fff;
	font-size: 25rpx;
}

