
.fb_li{
	width: 100%;
	padding: 0 28rpx;
	box-sizing: border-box;
	min-height: 100rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
